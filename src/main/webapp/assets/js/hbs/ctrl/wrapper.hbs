<div class="grid-stack-item" data-ctrl-id="{{id}}" data-gs-x="{{x}}" data-gs-y="{{y}}" data-gs-width="{{width}}" data-gs-height="{{height}}">
    <div class="grid-stack-item-content"></div>
    <div class="grid-stack-item-delete"><a role="button" class="close" aria-label="Delete" onclick="removeCtrl('{{id}}')"><span aria-hidden="true">&times;</span></a></div>
</div>
